<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/d"
    tools:context="com.example.lyndaob.reminderapplication.HomeActivity">
    <Button
        android:text="REMINDER"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/remBtn"
        android:layout_below="@+id/remImg"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:src="@drawable/cal"
        android:id="@+id/remImg"
        android:layout_marginTop="74dp"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/remBtn" />

    <Button
        android:text="TIMER"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/TimeBtn"
        android:layout_below="@+id/timeImg"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="NOTES"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nteImg"
        android:layout_centerHorizontal="true"
        android:id="@+id/NteBtn" />

    <ImageView
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:src="@drawable/peertalk"
        android:id="@+id/shareImg"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/remBtn"
        android:layout_alignEnd="@+id/remBtn" />

    <Button
        android:text="PEER TALK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/shareImg"
        android:id="@+id/shareBtn"
        android:layout_below="@+id/shareImg" />

    <Button
        android:text="SETTINGS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/settBtn"
        android:layout_below="@+id/shareImg"
        android:layout_alignStart="@+id/NteBtn" />

    <Button
        android:text="ABOUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/settBtn"
        android:layout_alignParentEnd="true"
        android:id="@+id/abtBtn"
        android:layout_below="@+id/settImg" />

    <ImageView
        android:layout_width="500dip"
        android:layout_height="wrap_content"
        android:src="@drawable/about"
        android:id="@+id/abtImg"
        android:layout_alignTop="@+id/shareImg"
        android:layout_alignStart="@+id/timeImg" />

    <ImageView
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:src="@drawable/h"
        android:id="@+id/homeImg"
        android:layout_marginBottom="24dp"
        android:layout_above="@+id/timeImg"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:src="@drawable/sett"
        android:id="@+id/settImg"
        android:layout_above="@+id/settBtn"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:src="@drawable/tim"
        android:id="@+id/timeImg"
        android:layout_alignTop="@+id/remImg"
        android:layout_alignStart="@+id/TimeBtn" />

    <ImageView
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:src="@drawable/notes"
        android:id="@+id/nteImg"
        android:layout_above="@+id/remBtn"
        android:layout_alignStart="@+id/settImg" />

</RelativeLayout>


